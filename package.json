{
  "name": "@nebula.js/sn-org-chart",
  "version": "1.0.3",
  "description": "Org chart supernova",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "supernova"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/qlik-oss/sn-org-chart/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-org-chart.git"
  },
  "files": [
    "dist",
    "api-specifications",
    "core",
    "sn-org-chart-ext"
  ],
  "engines": {
    "node": ">=16"
  },
  "main": "dist/sn-org-chart.js",
  "module": "dist/sn-org-chart.esm.js",
  "scripts": {
    "build": "node ./tools/build.js --core --ext",
    "build:dev": "node ./tools/build.js --core --ext --mode development",
    "build:watch": "node ./tools/build.js --ext --mode development -w",
    "sense": "nebula sense --ext ./src/extension/ext-raw.js --meta ./src/extension/meta.json",
    "lint": "eslint src",
    "start": "nebula serve --type sn-org-chart",
    "spec": "scriptappy-from-jsdoc -c ./spec-configs/props.conf.js",
    "test:unit": "jest",
    "test:integration": "aw puppet --glob 'test/integration/**/*.spec.js' --chrome.headless true --chrome.slowMo 10",
    "test:rendering": "aw puppet --testExt '*.spec.js' --glob 'test/rendering/**/*.spec.js' --mocha.bail false --mocha.timeout 30000",
    "prepublishOnly": "NODE_ENV=production yarn run build && yarn spec",
    "preversion": "yarn build",
    "version": "yarn spec && git add api-specifications"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "eslint"
    ]
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=2.10.0"
  },
  "devDependencies": {
    "@after-work.js/aw": "6.0.14",
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@nebula.js/cli": "2.10.0",
    "@nebula.js/cli-build": "2.10.0",
    "@nebula.js/cli-sense": "2.10.0",
    "@nebula.js/cli-serve": "2.10.0",
    "@nebula.js/stardust": "2.10.0",
    "babel-plugin-istanbul": "6.1.1",
    "d3": "5.16.0",
    "eslint": "8.15.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.0.4",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-junit": "13.2.0",
    "less": "4.1.2",
    "lint-staged": "12.4.1",
    "mocha-junit-reporter": "2.0.2",
    "scriptappy-from-jsdoc": "0.7.0",
    "shx": "0.3.4",
    "yargs": "17.5.0"
  }
}

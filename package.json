{
  "name": "@nebula.js/sn-org-chart",
  "version": "0.10.1",
  "description": "",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "supernova"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/qlik-oss/sn-org-chart/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-org-chart.git"
  },
  "files": [
    "assets",
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "main": "dist/sn-org-chart.js",
  "module": "dist/sn-org-chart.esm.js",
  "scripts": {
    "build": "nebula build --no-sourcemap && shx cp assets/* dist",
    "sense": "nebula sense --ext ./src/extension/ext-raw.js --meta ./src/extension/meta.json",
    "lint": "eslint src",
    "start": "nebula serve --type sn-org-chart",
    "test:unit": "aw -c aw.config.js",
    "test:integration": "aw puppet --glob 'test/integration/**/*.spec.js' --chrome.headless true --chrome.slowMo 10",
    "prepublishOnly": "rm -rf dist && npm run build",
    "version": "node ./scripts/version-qext.js && git add -A"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "eslint"
    ]
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=1.0.0"
  },
  "devDependencies": {
    "@after-work.js/aw": "6.0.13",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@nebula.js/cli": "1.0.0",
    "@nebula.js/cli-build": "1.0.0",
    "@nebula.js/cli-serve": "1.0.1",
    "@nebula.js/stardust": "1.0.2-alpha.0",
    "babel-plugin-istanbul": "6.0.0",
    "d3": "5.16.0",
    "eslint": "7.6.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-mocha": "7.0.1",
    "husky": "4.2.5",
    "less": "^3.12.2",
    "lint-staged": "10.2.11",
    "mocha-junit-reporter": "2.0.0",
    "shx": "0.3.2"
  }
}

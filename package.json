{
  "name": "@nebula.js/sn-org-chart",
  "version": "0.15.0",
  "description": "",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "supernova"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/qlik-oss/sn-org-chart/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-org-chart.git"
  },
  "files": [
    "assets",
    "dist"
  ],
  "engines": {
    "node": ">=12"
  },
  "main": "dist/sn-org-chart.js",
  "module": "dist/sn-org-chart.esm.js",
  "scripts": {
    "build": "nebula build --sourcemap false && shx cp assets/* dist",
    "sense": "nebula sense --ext ./src/extension/ext-raw.js --meta ./src/extension/meta.json",
    "lint": "eslint src",
    "start": "nebula serve --type sn-org-chart",
    "test:unit": "jest",
    "test:integration": "aw puppet --glob 'test/integration/**/*.spec.js' --chrome.headless true --chrome.slowMo 10",
    "test:rendering": "aw puppet --testExt '*.spec.js' --glob 'test/rendering/**/*.spec.js' --mocha.bail false --mocha.timeout 30000",
    "prepublishOnly": "rm -rf dist && npm run build",
    "version": "node ./scripts/version-qext.js && git add -A"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "eslint"
    ]
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=2.1.0"
  },
  "devDependencies": {
    "@after-work.js/aw": "6.0.14",
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@nebula.js/cli": "2.3.0",
    "@nebula.js/cli-build": "2.3.0",
    "@nebula.js/cli-sense": "2.3.0",
    "@nebula.js/cli-serve": "2.3.0",
    "@nebula.js/stardust": "2.3.0",
    "babel-plugin-istanbul": "6.1.1",
    "d3": "5.16.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-mocha": "9.0.0",
    "husky": "4.3.8",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "less": "^4.1.2",
    "lint-staged": "11.2.3",
    "mocha-junit-reporter": "2.0.2",
    "shx": "0.3.3"
  }
}

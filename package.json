{
  "name": "@nebula.js/sn-org-chart",
  "version": "1.1.0",
  "description": "Org chart supernova",
  "keywords": [
    "qlik",
    "nebula",
    "supernova"
  ],
  "bugs": {
    "url": "https://github.com/qlik-oss/sn-org-chart/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-org-chart.git"
  },
  "license": "MIT",
  "author": "QlikTech International AB",
  "main": "dist/sn-org-chart.js",
  "files": [
    "dist",
    "api-specifications",
    "core",
    "sn-org-chart-ext"
  ],
  "scripts": {
    "build": "yarn run locale:generate && node ./tools/build.js --core --ext",
    "build:dev": "yarn run locale:generate && node ./tools/build.js --core --ext --mode development",
    "build:watch": "yarn run locale:generate && node ./tools/build.js --ext --mode development -w",
    "format": "prettier --write '**'",
    "format:check": "prettier --check '**'",
    "lint": "eslint src",
    "locale:generate": "node src/locale/scripts/generate-all.mjs",
    "locale:verify": "node src/locale/tools/verify-translations.js",
    "prepublishOnly": "NODE_ENV=production yarn run build && yarn spec",
    "sense": "nebula sense --ext ./src/extension/ext-raw.js --meta ./src/extension/meta.json",
    "spec": "scriptappy-from-jsdoc -c ./spec-configs/props.conf.js",
    "start": "nebula serve --type sn-org-chart",
    "test:rendering": "playwright test",
    "test:unit": "jest",
    "preversion": "yarn build",
    "version": "yarn spec && git add api-specifications"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "eslint",
      "prettier --check src/**"
    ]
  },
  "prettier": "@qlik/prettier-config",
  "eslintConfig": {
    "extends": [
      "@qlik/eslint-config",
      "@qlik/eslint-config/jest"
    ],
    "rules": {
      "no-param-reassign": 0,
      "no-unused-expressions": 0,
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.20",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@nebula.js/cli": "4.4.0",
    "@nebula.js/cli-build": "4.4.0",
    "@nebula.js/cli-sense": "4.4.0",
    "@nebula.js/cli-serve": "4.4.0",
    "@nebula.js/stardust": "4.4.0",
    "@playwright/test": "1.38.0",
    "@qlik/eslint-config": "0.4.18",
    "@qlik/prettier-config": "0.3.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "d3": "7.8.5",
    "eslint": "8.49.0",
    "fs-extra": "11.1.1",
    "globby": "13.2.2",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "less": "4.2.0",
    "lint-staged": "14.0.1",
    "mocha-junit-reporter": "2.2.1",
    "prettier": "3.0.3",
    "scriptappy-from-jsdoc": "0.7.0",
    "shx": "0.3.4",
    "yargs": "17.7.2"
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=4.4.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "systemjs": "dist/sn-org-chart.systemjs.js"
}
